{
  "name": "@toruslabs/session-manager",
  "version": "4.0.2",
  "description": "session manager web",
  "sideEffects": false,
  "main": "dist/lib.cjs/index.js",
  "module": "dist/lib.esm/index.js",
  "unpkg": "dist/sessionManagerWeb.umd.min.js",
  "jsdelivr": "dist/sessionManagerWeb.umd.min.js",
  "types": "dist/lib.cjs/types/index.d.ts",
  "repository": "git@github.com:torusresearch/session-manager-web.git",
  "scripts": {
    "start": "cross-env FORCE_COLOR=1 torus-scripts start",
    "dev": "rimraf dist/ && cross-env NODE_ENV=development torus-scripts start",
    "build": "torus-scripts build",
    "release": "torus-scripts release",
    "lint": "eslint --fix 'src/**/*.ts'",
    "prepack": "npm run build",
    "pre-commit": "lint-staged --cwd .",
    "prepare": "husky",
    "test": "vitest run --config=test/config/node.config.mts --coverage"
  },
  "dependencies": {
    "@toruslabs/constants": "^14.2.0",
    "@toruslabs/eccrypto": "^6.0.2",
    "@toruslabs/http-helpers": "^8.1.0",
    "@toruslabs/metadata-helpers": "^7.0.2"
  },
  "devDependencies": {
    "@babel/register": "^7.25.9",
    "@babel/runtime": "^7.26.9",
    "@toruslabs/config": "^3.1.0",
    "@toruslabs/eslint-config-typescript": "^4.1.0",
    "@toruslabs/torus-scripts": "^7.1.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22",
    "@vitest/coverage-istanbul": "^3.0.8",
    "@vitest/coverage-v8": "^3.0.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.35.0"
  },
  "author": "Torus Labs",
  "license": "MIT",
  "lint-staged": {
    "!(*d).{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.x",
    "npm": ">=9.x"
  },
  "overrides": {
    "esbuild": "^0.25.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
