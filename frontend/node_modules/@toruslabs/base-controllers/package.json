    "async-mutex": "^0.5.0",
    "bignumber.js": "^9.1.2",
    "bowser": "^2.11.0",
    "jwt-decode": "^4.0.0",
    "loglevel": "^1.9.2"
  },
  "devDependencies": {
    "@types/readable-stream": "^4.0.18"
  },
  "peerDependencies": {
    "@babel/runtime": "7.x"
  },
  "files": [
    "dist"
